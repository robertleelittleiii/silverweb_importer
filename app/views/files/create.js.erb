<% if @file.new_record? %>
  alert("Failed to upload file <%= j @file.errors.full_messages.join(', ').html_safe %>");
<% else %>
  $("div.filesection").append("<%= j render(:partial=>"/files/file.html") %>");

  $($("div#<%=@file.id%>.file-list-item div.file-item")).find("#logo-links").bind("click",
  function(){
  var href = $($(this)[0]).find('a').attr('href');
  window.location.href = href
  console.log($($(this)[0]).find('a').attr('href'));
  });
<% end %>